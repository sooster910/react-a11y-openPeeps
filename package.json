{
  "name": "react-accessbile-avatarpeeps",
  "version": "1.0.0",
  "description": "react a11y svg component librayry with openPeeps avartar  ",
  "main": "dist/index.js",
  "author": "Hyunsu Joo",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "start-storybook -p 6006",
    "prebuild": "rimraf src & rimraf dist",
    "build": "npm run svgr && rollup -c",
    "clean": "rimraf avatars/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "svgr": "svgr --icon --title-prop --replace-attr-values '#000={props.color}', --typescript -d src/avatars assets",
    "format": "prettier --write '*.js'",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "openPeeps",
    "Avatar",
    "a11y",
    "Svg",
    "React"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-controls": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@svgr/cli": "^5.5.0",
    "@svgr/rollup": "^5.5.0",
    "@types/react": "^17.0.19",
    "babel-loader": "^8.2.2",
    "eslint": "^8.1.0",
    "prettier": "2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.0",
    "react": "^17.0.2"
  }
